	body
		.w3-top
			#navbar.w3-bar
				a.w3-bar-item.w3-button.w3-hover-black.w3-hide-medium.w3-hide-large.w3-right(href='javascript:void(0);' onclick='toggle()' title='Toggle Navigation Menu')
					i.fas.fa-bars
				a.w3-bar-item.w3-button(href='/#home') HOME
				a.w3-bar-item.w3-button.w3-hide-small(href='/#about')
					i.fas.fa-user
					|  ABOUT
				a.w3-bar-item.w3-button.w3-hide-small(href='/#portfolio')
					i.fas.fa-th
					|  PORTFOLIO
				a.w3-bar-item.w3-button.w3-hide-small(href='/#contact')
					i.fas.fa-envelope
					|  CONTACT
			// Navbar on small screens
			#navDemo.w3-bar-block.w3-dark-grey.w3-hide.w3-hide-large.w3-hide-medium
				a.w3-bar-item.w3-button(href='#about' onclick='toggleFunction()') ABOUT
				a.w3-bar-item.w3-button(href='#portfolio' onclick='toggleFunction()') PORTFOLIO
				a.w3-bar-item.w3-button(href='#contact' onclick='toggleFunction()') CONTACT
		block content
