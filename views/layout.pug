		link(rel='icon' type='image/png' sizes='192x192' href='/icons/android-chrome-192x192.png?v=9BP4YaB3x3')
		link(rel='icon' type='image/png' sizes='16x16' href='/icons/favicon-16x16.png?v=9BP4YaB3x3')
