RewriteEngine on

RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://www.julianvos.nl/$1 [R,L]

Options -Indexes

RewriteCond %{HTTP_REFERER} !^$
RewriteCond %{HTTP_REFERER} !^http://julianvos.nl/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^http://julianvos.nl$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.julianvos.nl/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.julianvos.nl$      [NC]
RewriteCond %{HTTP_REFERER} !^https://julianvos.nl/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://julianvos.nl$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.julianvos.nl/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.julianvos.nl$      [NC]
RewriteRule .*\.()$ - [F,NC]

